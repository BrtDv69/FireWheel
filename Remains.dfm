  object ibdsMainDetails: TIBDataSet
    Database = frmDM.dbMain
    Transaction = trMain
    BufferChunks = 100
    CachedUpdates = True
    SelectSQL.Strings = (
      
        'select TR.STORE_TYPE, TR.STORE_ID, Cast('#39#1057#1082#1083#1072#1076#39' as VARCHAR(127))' +
        ' NAME, TR.TOTAL, TR.PRICE,'
      'TR.MIN_IN_DATE, TR.MAX_IN_DATE'
      'from TMP_REMAINS TR'
      
        'where (TR.ARTICUL_ID=:ARTICUL_ID) and (TR.CREATOR=current_user) ' +
        'and (TR.STORE_TYPE=0) and'
      '(TR.STORE_ID=0)'
      'union'
      'select TR.STORE_TYPE, TR.STORE_ID,'
      
        'Cast('#39#1055#1086#1076#1086#1090#1095#1077#1090#1085#1080#1082#39'||'#39' '#39'||P.SURNAME||'#39' '#39'||P.NAME||'#39' '#39'||P.SECOND_N' +
        'AME as VARCHAR(127)) NAME,'
      'TR.TOTAL, TR.PRICE, TR.MIN_IN_DATE, TR.MAX_IN_DATE'
      'from TMP_REMAINS TR, PERSONS P'
      
        'where (TR.ARTICUL_ID=:ARTICUL_ID) and (TR.CREATOR=current_user) ' +
        'and (TR.STORE_TYPE=1) and'
      '(TR.STORE_ID=P.ID)'
      'union'
      'select TR.STORE_TYPE, TR.STORE_ID,'
      
        'Cast('#39#1042#1086#1076#1080#1090#1077#1083#1100#39'||'#39' '#39'||P.SURNAME||'#39' '#39'||P.NAME||'#39' '#39'||P.SECOND_NAME' +
        ' as VARCHAR(127)) NAME,'
      'TR.TOTAL, TR.PRICE, TR.MIN_IN_DATE, TR.MAX_IN_DATE'
      'from TMP_REMAINS TR, PERSONS P'
      
        'where (TR.ARTICUL_ID=:ARTICUL_ID) and (TR.CREATOR=current_user) ' +
        'and (TR.STORE_TYPE=2) and'
      '(TR.STORE_ID=P.ID)'
      'union'
      'select TR.STORE_TYPE, TR.STORE_ID,'
      
        'Cast('#39#1040#1074#1090#1086#1084#1086#1073#1080#1083#1100#39'||'#39' '#39'||AM.NAME||'#39' '#39'||AU.STATE_NUMBER as VARCHAR' +
        '(127)) NAME,'
      'TR.TOTAL, TR.PRICE, TR.MIN_IN_DATE, TR.MAX_IN_DATE'
      'from TMP_REMAINS TR, AUTOMOBILES AU, AUTOMOBILE_MAKES AM'
      
        'where (TR.ARTICUL_ID=:ARTICUL_ID) and (TR.CREATOR=current_user) ' +
        'and (TR.STORE_TYPE=3) and'
      '(TR.STORE_ID=AU.ID) and (AU.AUTOMOBILE_MAKE_ID=AM.ID)'
      'order by 1, 3, 5')
